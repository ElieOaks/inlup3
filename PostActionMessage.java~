public class PostActionMessage {
    private PostAction action;
    private int globalPostId;
    protected Post post;
    
    public PostActionMessage(PostAction action, int globalPostId) {
        this.action = action;
        this.globalPostId = globalPostId;
    }

    public PostAction getAction() {
        return this.action;
    }

    public Post getPost() {
        return this.post;
    }

}
